{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "Node",
		"declaration": true,
		"outDir": "./build",
		"strict": true,
		"removeComments": true,
		"jsx": "react-jsx",
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["./src/**/*.ts*"]
}
